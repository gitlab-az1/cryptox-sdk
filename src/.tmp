const chars = text.toLowerCase().split('');
    const vectorSize = embeddingModel.embeddingSize;
    let vector = Array.from({ length: vectorSize }, () => 0);

    for (const char of chars) {
        if (embeddingModel.charToIndex.has(char)) {
            const charVector = embeddingModel.getCharVector(char);

            for (let i = 0; i < vectorSize; i++) {
                vector[i] += charVector[i];
            }
        } else {
            console.log(`Character not found in the model: ${char}`);
        }
    }